{
    "extends": ["airbnb-base", "prettier"],
    "rules": {
      "import/prefer-default-export": "off",
      "class-methods-use-this": "off",
      "linebreak-style": ["error", "unix"],
      "quotes": ["error", "single", { "avoidEscape": true }],
      "no-console": "error"
    },
    "plugins": ["prettier", "html"]
  }